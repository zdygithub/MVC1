
@{
    ViewBag.Title = "List";
    var newList = ViewBag.data as string[];

}

<h2>List</h2>

<ol>
    @for (int i = 0; i < Model.Length; i++)
    {
        <li>@Model[i]</li>
    }
</ol>

<ol class="list-group">
    @foreach (var item in newList)
    {
        <li class="list-group-item"><span class="badge">新</span>@item</li>
    }
</ol>

